<h1>Account Info</h1>

<p><%= @user.username %></p>
<p><%= @user.email %></p>
<% @listings.each do |listing|%>
  <p><%= listing.name %></p>
  <p><%= listing.description %></p>
  <p><%= listing.price %></p>
  <% listing.pictures.each do |picture| %>
    <img src="<%= picture["url"] %>" alt="">
  <% end %>
<% end %>
